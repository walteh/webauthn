root = true

[*]
indent_style = tab
indent_size = 4
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
trim_multiple_empty_lines = true



[*.md]
trim_trailing_whitespace = true

[*.txt]
trim_trailing_whitespace = true
insert_final_newline = false

[*.yml]
indent_style = space
indent_size = 4

[*.proto]
indent_style = tab
indent_size = 4
